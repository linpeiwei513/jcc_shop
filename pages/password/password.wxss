/* pages/login/login.wxss */
page{
  background-color: #f9deb7;
}
.lo-main{
  width: 490rpx;
  margin: 0rpx auto;
  margin-top: 100rpx;
}
.lo-put{
  margin-bottom: 30rpx;
}
.lo-put input{
  width: 100%;
  background-color: #ffffff;
  height: 90rpx;
  text-align: center;
  font-size: 34rpx;
  border-radius: 10rpx;
}
.lo-logo{
  width: 170rpx;
  height: 170rpx;
  margin: 0rpx auto;
  margin-bottom: 100rpx;
}
.lo-logo image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  text-align: center;
}
.lo-btn{
  margin-top: 30rpx;
  padding-top: 30rpx;
}
.lo-btn button{
  width: 490rpx;
  font-size: 34rpx;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0rpx;
  color: #ffffff;
  background-color: #f75060;
  
}