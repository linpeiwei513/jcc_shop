.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background-color: #f9f9f9;
}
.co{
  clear: both;
}
.le{
  float: left;
}
.rt{
  float: right;
}
.none{
  text-align: center;
  font-size: 32rpx;
  color: #888888;
  margin-top: 120rpx;
}
.none image{
  width: 160rpx;
  height: 160rpx;
}